<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true"
assembly="NHibernateApplicationMVC" namespace="NHibernateApplicationMVC.Models">
	<class name="EmployeeDetails" table="EmployeeDetails" dynamic-update="true" >
		<cache usage="read-write"/>
		<id name="Id" column="Id" type="int">
			<generator class="native" />
		</id>
		<property name="Name"  />
		<property name="Address" />
		<property name="MobileNo"  />
		<property name="BirthDate"/>
		<property name="BloodGroup" />
		<property name="EmergencyNo"  />
		<property name="AddedOn" />
		<property name="UpdatedOn" />
		<property name="Isactive"/>
	</class>
</hibernate-mapping>

